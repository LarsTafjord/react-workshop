{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": true,
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "sourceMap": true,
    "jsx": "react",
    "rootDir": "src",
    "baseUrl": "src",
    "allowSyntheticDefaultImports": true,
  },
  "exclude":  ["node_modules"]
}